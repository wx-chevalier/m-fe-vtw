{
  "author": "wx-chevalier@github",
  "description": "vtw-components",
  "license": "MIT",
  "keywords": [
    "webpack",
    "react"
  ],
  "name": "vtw-components",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/wx-chevalier/fe-boilerplates"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{ts,tsx,scss,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run build:es && npm run build:cjs && npm run build:umd",
    "build:es": "tsc --project ./tsconfig.es.json",
    "build:cjs": "tsc --project ./tsconfig.cjs.json",
    "build:umd": "NODE_ENV=production webpack --config ./scripts/webpack/webpack.config.umd.js",
    "lint": "tsc --noEmit && eslint '*/**/*.{ts,tsx}' --quiet --fix",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "antd": "^3.21.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "vtw-core": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-info": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@types/classnames": "^2.2.9",
    "@types/jest": "24.0.17",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/redux-actions": "^2.6.1",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/storybook__react": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "autoprefixer": "9.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "3.1.0",
    "enzyme": "^3.10.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "4.1.0",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "json-server": "0.15.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "parallelshell": "^3.0.2",
    "postcss-loader": "3.0.0",
    "prettier": "^1.18.2",
    "react-docgen-typescript-loader": "^3.1.1",
    "react-hot-loader": "^4.12.10",
    "react-router-dom": "^5.0.1",
    "resolve-url-loader": "3.1.0",
    "rimraf": "^2.6.3",
    "style-loader": "0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "6.0.4",
    "typescript": "3.5.3",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "^2.1.0",
    "wasm-loader": "^1.3.0",
    "webpack": "^4.39.1",
    "webpack-cli": "3.3.6",
    "webpack-dashboard": "3.0.7",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1",
    "workerize-loader": "^1.0.4"
  },
  "files": [
    "dist/"
  ]
}
