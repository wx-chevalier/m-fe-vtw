    "redux-thunk": "^2.3.0",
    "smoothscroll-polyfill": "^0.4.4",
    "umi-request": "^1.2.3",
    "vtw-components": "*",
    "vtw-core": "*",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.2.2",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "24.0.18",
    "@types/lodash": "^4.14.137",
