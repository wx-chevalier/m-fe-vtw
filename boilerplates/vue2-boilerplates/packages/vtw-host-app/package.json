{
  "author": "wx-chevalier@github",
  "description": "vtw-host-app",
  "license": "MIT",
  "keywords": [
    "webpack",
    "react"
  ],
  "name": "vtw-host-app",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/wx-chevalier/fe-boilerplates"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{ts,tsx,scss,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "prebuild": "npm run clean && npm run cleanCov",
    "clean": "rimraf dist/*",
    "cleanCov": "rimraf @coverage",
    "build": "NODE_ENV=production webpack --config ./scripts/webpack/webpack.config.prod.js",
    "build:umd": "NODE_ENV=production webpack --config ./scripts/webpack/webpack.config.umd.js",
    "dev": "webpack-dev-server --config ./scripts/webpack/webpack.config.dev.js --hot",
    "dev:umd": "webpack-dev-server --config ./scripts/webpack/webpack.config.umd.dev.js --hot",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "jest --config ../../scripts/jest/jest.config.js",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm run cleanCov && npm test -- --coverage",
    "test:update": "npm test -- --updateSnapshot",
    "start": "npm run dev"
  },
  "dependencies": {
    "@ant-design/pro-layout": "^4.5.14",
    "antd": "^3.22.1",
    "axios": "^0.19.0",
    "connected-react-router": "^6.5.2",
