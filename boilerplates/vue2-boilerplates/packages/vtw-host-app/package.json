    "awesome-typescript-loader": "^5.2.1",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "3.2.0",
    "enzyme": "^3.10.0",
    "eslint": "^6.2.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "3.0.0",
